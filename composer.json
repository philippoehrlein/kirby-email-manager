{
  "name": "philippoehrlein/kirby-email-manager",
  "description": "A Kirby plugin for managing email templates",
  "type": "kirby-plugin",
  "version": "1.0.0",
  "license": "proprietary",
  "homepage": "https://github.com/philippoehrlein/kirby-email-manager",
  "authors": [
    {
      "name": "Philip Poehrlein",
      "email": "github@philippoehrlein.com"
    }
  ],
  "require": {
    "php": ">=8.1.0"
  },
  "require-dev": {
    "getkirby/cms": "^4.0"
  },
  "autoload": {
      "psr-4": {
          "KirbyEmailManager\\": "src/"
      }
  },
  "extra": {
      "kirby-cms-path": "../../../kirby"
  },
  "config": {
    "allow-plugins": {
      "getkirby/composer-installer": true
    }
  }
}